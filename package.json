{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-p build:*",
    "build:remix": "remix build",
    "build:css": "tailwindcss -m -i app/styles/tailwind.css -o app/styles/tailwind.generated.css",
    "dev": "run-p dev:*",
    "dev:remix": "remix dev",
    "dev:css": "tailwindcss -w -i app/styles/tailwind.css -o app/styles/tailwind.generated.css",
    "start": "remix-serve build",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@remix-run/node": "^1.14.3",
    "@remix-run/react": "^1.14.3",
    "@remix-run/serve": "^1.14.3",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.4.0",
    "remix-auth-form": "^1.3.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.14.3",
    "@remix-run/eslint-config": "^1.14.3",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.11.0",
    "remix-flat-routes": "^0.5.8",
    "tailwind-children": "^0.5.0",
    "tailwind-saasblocks": "^0.2.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
