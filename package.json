{
  "private": true,
  "sideEffects": false,
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:css": "npm run generate:css",
    "build:remix": "remix build",
    "build:seed": "npm run seed:database",
    "dev": "run-p dev:*",
    "dev:remix": "remix dev",
    "dev:css": "npm run generate:css -- --watch",
    "dev:seed": "npm run seed:database",
    "dev:webhooks-stripe": "hookdeck listen 3000 sm -p dev",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.generated.css",
    "seed:database": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "@prisma/client": "^4.11.0",
    "@remix-run/node": "^1.14.3",
    "@remix-run/react": "^1.14.3",
    "@remix-run/serve": "^1.14.3",
    "@stripe/react-stripe-js": "^2.0.0",
    "@stripe/stripe-js": "^1.49.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.4.0",
    "dayjs": "^1.11.7",
    "domain-functions": "^1.6.1",
    "isbot": "^3.6.5",
    "jsonwebtoken": "^9.0.0",
    "postmark": "^3.0.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.7",
    "remix-auth": "^3.4.0",
    "remix-auth-form": "^1.3.0",
    "remix-forms": "^1.6.1",
    "remix-utils": "^6.0.0",
    "stripe": "^11.15.0",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.14.3",
    "@remix-run/eslint-config": "^1.14.3",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "prisma": "^4.11.0",
    "remix-flat-routes": "^0.5.8",
    "tailwind-children": "^0.5.0",
    "tailwind-saasblocks": "^0.2.0",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14"
  }
}
